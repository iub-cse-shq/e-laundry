            <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">              
                <ul class="nav nav-pills nav-stacked nav-account">
                    <li class="active"><a href="#">My Account</a></li>
                    <li><a id="profile" href="#">Profile</a></li>
                    <li><a  id="mypackage" href="#">My Package</a></li>
                    
                    <li><a id="orderinfo" href="#">Order Information</a></li>
                    
                    <li><a href="index.html">Logout</a></li>
                </ul>
            </div>
        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12" id="order">
            <h4 class="headline">Order Information</h4>
            <table class="table table-bordered table-condensed">
                <thead>
                    <tr>
                       
                        <th>Order Name</th>
                    
                        <th>Contact No</th>
                        <th>Address</th>
                        <th>Area</th>
                        <th>Order Date</th>
                        <th>Order Time</th>
                        <th>Package</th> 
                        <th>Amount</th>
                        
                    </tr>                   
                </thead>
                     <tr>
                        <td><%= request.order.name %></td>
                        <td><%= request.order.contactno %></td>
                        <td><%= request.order.address %></td>
                        <td><%= request.order.area %></td>
                        <td><%= request.order.pickupDate %></td>
                        <td><%= request.order.pickupTime %></td>
                        <td><%= request.order.purchagePackage.name%></td>
                        <td><%= request.order.purchagePackage.price%></td>
                      </tr>
            </table>
            
        </div>

        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12" id="pr">
            <div class="col-lg-5 col-md-5 col-sm-5 col-xs-12">
                    <h4 class="headline">Profile Information</h4>
            <form class="form-horizontal ajax-form" action="/customer/profileUpdate" method="post">

                <div id="profile-message"></div>

                <div class="form-group">
                    <label for="CustEmail" class="col-sm-3 control-label">Email</label>
                    <div class="col-sm-6">
                        <span class="help-block">imranhasancs@gmail.com</span>
                    </div>
                </div>
                <div class="form-group">
                    <label for="CustName" class="col-sm-3 control-label">Name</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" id="CustName" name="CustName" value=<%=request.order.name%> placeholder="Full Name">
                    </div>
                </div>              
                <div class="form-group">
                    <label for="CustMobile" class="col-sm-3 control-label">Contact No</label>
                    <div class="col-sm-6">
                        <input type="text" class="form-control" id="CustMobile" name="CustMobile" value=<%=request.order.contactno%> placeholder="Contact No">
                    </div>
                </div>
                <div class="form-group">
                    <label for="CustAddress" class="col-sm-3 control-label">Address</label>
                    <div class="col-sm-6">
                        <textarea class="form-control" name="CustAddress" id="CustAddress" value=<%=request.order.contactno%> placeholder="Address">block-B</textarea>
                    </div>
                </div>
                <div class="form-group">
                    <label for="CustArea" class="col-sm-3 control-label">Area</label>
                    <div class="col-sm-6">
                        <select class="form-control" name="CustArea"  value=<%=request.order.area%> id="CustArea">
                            <option value="">Select Area</option>
                            <option value="2">Gulshan</option><option value="3">Banani</option><option value="4">Uttara</option><option value="6">Bashundhara</option><option value="7">Baridhara</option><option value="8">Khilkhet/Nikunjo</option>                                           
                        </select>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-sm-offset-3 col-sm-10">
                        <button type="submit" class="btn btn-primary">Update</button>
                    </div>
                </div>
            </form>
        </div>

        <div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
            <h4 class="headline">Change Password</h4>
            <form class="form-horizontal ajax-form" action="/customer/changePassword" method="post">

                <div id="password-message"></div>

                <div class="form-group">
                    <label for="Password" class="col-sm-5 control-label">Password</label>
                    <div class="col-sm-7">
                        <input type="password" class="form-control" id="Password" name="Password" placeholder="Password">
                    </div>
                </div>
                <div class="form-group">
                    <label for="ConfirmPassword" class="col-sm-5 control-label">Confirm Password</label>
                    <div class="col-sm-7">
                        <input type="password" class="form-control" id="ConfirmPassword" name="ConfirmPassword" placeholder="Confirm Password">
                    </div>
                </div>                              
                <div class="form-group">
                    <div class="col-sm-offset-3 col-sm-10">
                        <button type="submit" class="btn btn-primary">Change Password</button>
                    </div>
                </div>
            </form>
        </div>
        </div>

        <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12" id="package">
            <h4 class="headline">My Package</h4>
            <table class="table table-bordered table-condensed">
                <tbody>
                    <tr>
                        
                        <th rowspan="2" class="text-center">Date</th>
                        
                        <th rowspan="2" class="text-center">Package</th>                                                                           
                        <th colspan="3" class="text-center">Qty</th>
                        <th rowspan="2" class="text-center">Price</th>
                        <th rowspan="2" class="text-center">Paid</th>       
                        
                    </tr>
                    <tr>
                        <th class="text-center">Limit</th>
                        <th class="text-center">Used</th>
                        <th class="text-center">Remain</th>
                    </tr>   
                </tbody>
            </table>
        </div>
        

</div>
